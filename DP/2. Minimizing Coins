#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,am;
    cin>>n>>am;
    vector<int> coins;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        coins.push_back(a);
    }
    
    vector<vector<int>> dp(n,vector<int>(am+1,0));
    
    for(int i=0;i<=am;i++){
        if(i % coins[0] == 0){
            dp[0][i] = i / coins[0];
        }
        else{
            dp[0][i] = 1e9;
        }
    }
    
    for(int i=1;i<n;i++){
        for(int j=0;j<=am;j++){
            int nopick = dp[i-1][j];
            int pick = 1e9;
            
            if(j >= coins[i]){
                pick = 1 + dp[i][j-coins[i]];
            }
            
            dp[i][j] = min(pick,nopick);
        }
    }

    int ans = dp[n-1][am];
    
    if(ans == 1e9) cout<<-1<<endl;
    else cout<<ans<<endl;
    return 0;
}
